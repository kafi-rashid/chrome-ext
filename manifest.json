{
	"manifest_version": 2,
	"name": "BDERL",
	"description": "Bangladesh Digital Education Research, Limited",
	"version": "1",
	"author": "Mohammad Ali",
	"browser_action": {
		"default_icon": "tab-icon.png",
		"default_title": "BDERL"
	},
	"chrome_url_overrides": {
		"newtab": "newtab.html"
	},
	"permissions": [
		"https://*.gstatic.com/*",
		"activeTab"
	],
	"content_security_policy":"script-src 'self' https://www.gstatic.com/ https://*.firebaseio.com https://www.googleapis.com; object-src 'self'"
}
